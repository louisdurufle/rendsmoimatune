Options +FollowSymLinks
RewriteEngine On
RewriteBase /Rendsmoimatune/
RewriteRule    ^sign-in.html$                                                        sign-in.php [L]
RewriteRule    ^sign-out.html$                                                       sign-out.php [L]
RewriteRule    ^new-account.html$                                                    new-account.php [L]

# Dashboard
RewriteRule    ^dashboard/$                                                          index.php  [L]

# Events
RewriteRule    ^events/$                                                             events/index.php  [L]
RewriteRule    ^events/create-new-event.html$                                        events/create-new-event.php [L]
RewriteRule    ^events/[a-z0-9-]*-([0-9]+)/$                                         events/event.php?id=$1 [L]
RewriteRule    ^events/[a-z0-9-]*-([0-9]+)/create-new-expenditure.html$              events/create-new-expenditure.php?event-id=$1 [L]
RewriteRule    ^events/[a-z0-9-]*-[0-9]+/delete-[a-z0-9-]*-([0-9]+).html$            events/delete-expenditure.php?expenditure-id=$1 [L]


# My Account
RewriteRule    ^my-account/$                                                         my-account/index.php  [L]
RewriteRule    ^my-account/merge-user-([0-9]+)-with-([0-9]+).html$                   my-account/mergeUsers.php?id1=$1&id2=$2 [L]
